<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.preference.PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/pref_dev_opts_title">

    <android.support.v7.preference.ListPreference
        android:key="@string/pref_key_api_environment"
        android:title="@string/pref_api_env_title"
        android:selectable="true"
        android:enabled="true"
        android:order="100" />

    <com.codepunk.codepunklibstaging.preference.YesNoPreference
        android:key="@string/pref_key_disable_dev_opts"
        android:title="@string/pref_dev_mode_disable_dialog_title"
        android:dialogMessage="@string/pref_dev_mode_disable_dialog_message"
        android:positiveButtonText="@android:string/ok"
        android:negativeButtonText="@android:string/cancel"
        android:persistent="false"
        android:selectable="true"
        android:enabled="true"
        android:order="200" />

</android.support.v7.preference.PreferenceScreen>
